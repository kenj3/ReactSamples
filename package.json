{
  "name": "admin",
  "version": "1.0.0",
  "description": "运去哪后台管理系统adminV3",
  "scripts": {
    "start": "better-npm-run start:dev",
    "build": "better-npm-run build",
    "lint": "eslint src/app",
    "stylelint": "stylelint src/app/*/**.less --syntax less"
  },
  "betterScripts": {
    "start:dll": {
      "command": "webpack  --config ./dev-config/webpack.dll",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:dev": {
      "command": "node ./dev-config/webpack.server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config ./dev-config/webpack.prd",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "ray",
